{
  "name": "lock-in",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "test": "vitest",
    "dev": "tsx --watch src/server.ts",
    "build": "esbuild src/server.ts --bundle --platform=node --format=esm --outdir=dist --packages=external",
    "types": "tsc --noEmit",
    "format": "prettier --write . --ignore-path .prettierignore",
    "db:generate": "npx drizzle-kit generate",
    "db:custom": "npx drizzle-kit generate --custom",
    "db:migrate": "npx drizzle-kit migrate",
    "token": "tsx --env-file .env src/infra/auth/auth.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "bash -c 'tsc --noEmit'"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*.{ts,tsx,js,jsx,json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@sentry/node": "^10.38.0",
    "@supabase/supabase-js": "^2.94.1",
    "bullmq": "^5.67.3",
    "chalk": "^5.6.2",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "meebo": "^0.5.6",
    "postgres": "^3.4.8",
    "stripe": "^20.3.1",
    "winston": "^3.19.0",
    "winston-transport": "^4.9.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.0",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "drizzle-kit": "^0.31.8",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "3.8.1",
    "supertest": "^7.2.2",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  }
}
